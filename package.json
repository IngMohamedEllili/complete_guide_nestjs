    "@nestjs/core": "^8.0.0",
    "@nestjs/cqrs": "^8.0.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.5",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.44",
    "@types/uuid": "^8.3.4",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cross-env": "^7.0.3",
    "event-sourcing-nestjs": "^1.1.4",
    "eventstore": "^1.15.5",
    "joi": "^17.5.0",
    "mysql": "^2.18.1",
    "nestjs-eventstore": "git+https://github.com/AbdessalemLetaief/nestjs-eventstore",
    "node-eventstore-client": "^0.2.18",
    "node-nats-streaming": "^0.3.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "psx-nestjs-eventstore": "^1.4.16",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize-typescript": "^2.1.2",
